<div class="main posts-index">
  <div class="container-fluid">
    <div class="text-center"><h3>投稿一覧</h3></div><br>

    <div class="imageIndex">
      <% @posts.each do |post| %>
        <% p = post.photos.find_by(post_id: post) %>
        <%= link_to image_tag("#{p.image}", class: "img-responsive cover"), post_path(post) %>
        <% end %>
    </div>

  </div>
</div>
