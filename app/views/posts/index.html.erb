<div class="main posts-index">
  <div class="container-fluid">
    <div class="text-center"><h3>投稿一覧</h3></div>

      <table class="table">
        <thead>
          <tr>
                <th class="col-xs-1"></th>
                <th class="col-xs-1"></th>
                <th class="col-xs-1"></th>
                <th class="col-xs-1"></th>
                <th class="col-xs-1"></th>
                <th class="col-xs-1"></th>
              </tr>
            </thead>
            <tbody>
              <% @posts.each do |post| %>
              <tr>
                <td><%= link_to image_tag("#{post.photos.image}"), post_path(post) %></td>
              </tr>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>

  </div>
</div>
